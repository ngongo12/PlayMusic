<?xml version="1.0" encoding="UTF-8" ?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android" >
    <item
        android:id="@+id/progressshape" >
        <clip>
            <shape
                android:shape="rectangle" >
                <corners
                    android:radius="50dp" />
                <solid android:color="@color/light_blue"/>
